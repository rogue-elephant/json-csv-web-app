{
  "name": "json-csv-web-app",
  "version": "1.0.0",
  "description": "Functions for converting JSON objects to CSV as well as possibility to choose specific fields to convert.",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogue-elephant/json-csv-web-app.git"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "test": "jest --config jestconfig.json",
    "parcel": "parcel build src/public/scripts/main.ts -d dist/public/js",
    "build": "npm-run-all clean lint tsc copy-assets parcel",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "tsc": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "keywords": [
    "json",
    "csv"
  ],
  "author": "Tom Randell",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.2",
    "@types/shelljs": "^0.8.5",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "json-csv-tool": "^1.1.5",
    "vue": "^2.6.10"
  }
}
